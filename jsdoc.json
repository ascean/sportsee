{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["."],
        "exclude": [ "node_modules/" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "default": {
            "staticFiles": {
                "include": [ "./sportsee/public/images" ]
            }
        },
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "recurse":true,
        "destination":"./docs/",
        "readme":"./readme.md"
    }
}